<table class="govuk-table">
  <caption class="govuk-table__caption"><%= table_type %></caption>
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th scope="col" class="govuk-table__header">Ref</th>
      <th scope="col" class="govuk-table__header">Opted in</th>
      <th scope="col" class="govuk-table__header">Access count</th>
       <th scope="col" class="govuk-table__header">Created at</th>
      <th scope="col" class="govuk-table__header">Updated at</th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    <% results.each do |participant| %>
      <%= render partial: 'participant_row', locals: { participant: participant } %>
    <% end %>
  </tbody>
</table>
