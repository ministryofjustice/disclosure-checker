<table class="govuk-table">
  <caption class="govuk-table__caption"><%= table_type %></caption>
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th scope="col" class="govuk-table__header govuk-!-width-one-half">Ref</th>
      <th scope="col" class="govuk-table__header govuk-!-width-one-quarter">Opted in</th>
      <th scope="col" class="govuk-table__header govuk-!-width-one-quarter">Access count</th>
       <th scope="col" class="govuk-table__header govuk-!-width-one-half">created_at</th>
      <th scope="col" class="govuk-table__header govuk-!-width-one-half">updated_at</th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    <% results.each do |participant| %>
      <%= render partial: 'participant_row', locals: {participant: participant} %>
    <% end %>
  </tbody>
</table>
